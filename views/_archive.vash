
<hr />
<h3>archive</h3>
<div class="archive">
@model.yearAndPosts.forEach(function(item){
  <strong>@item.year</strong>
  <br />
      @item.posts.forEach(function(p){
        <p title="@p.description">
          <a href="/post/@p.name">
            <small>@p.header</small>
          </a>
        </p>
      })
})
</div>
